@if (TempData["success"] != null)
{
    <script type="text/javascript">
        toastr.success('@TempData["success"]');   @* Note: '' is important =) *@
    </script>
}

@if (TempData["error"] != null)
{
    <script type="text/javascript">
        toastr.error('@TempData["error"]');
    </script>
}
